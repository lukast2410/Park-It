<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tiffanytiph.com.parkit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ParkItWithToolbar">
        <activity android:name=".SearchActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".BookingDetailActivity" />
        <activity android:name=".CheckoutActivity" />
        <activity android:name=".BookingSlotActivity" />
        <activity android:name=".BookingActivity" />
        <activity android:name=".AccountActivity" />
        <activity android:name=".ParkingHistoryActivity" />
        <activity
            android:name=".RegisterActivity"
            android:theme="@style/Theme.ParkIt" />
        <activity
            android:name=".LoginActivity"
            android:theme="@style/Theme.ParkIt" />
        <activity
            android:name=".SplashScreen"
            android:theme="@style/Theme.ParkIt">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>